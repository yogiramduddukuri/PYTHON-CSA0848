leap year...   year1=1947
year=int(input("enter the year:"))

if (year % 400 == 0) and (year1 % 100 == 0):
    print("{0} is a leap year1".format(year))

elif (year % 4 ==0) and (year1 % 100 != 0):
    print("{0} is a leap year1".format(year))

else:
    print("{0} is not a leap year1".format(year))
